generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "postgresql"
}

model Document {
  id        String   @id @default(cuid())
  title     String
  content   String
  url       String   @unique
  scrapedAt DateTime @default(now())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([url])
}
